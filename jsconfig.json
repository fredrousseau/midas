{
	"compilerOptions": {
		"module": "ES2022",
		"target": "ES2022",
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"#utils/*": ["src/Utils/*"],
			"#trading/*": ["src/Trading/*"],
			"#data/*": ["src/Data/*"],
			"#logger": ["src/Logger/LoggerService.js"],
			"#mcp/*": ["src/Mcp/*"]
		}
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
